# AI Generation Settings - Copy to settings.env and fill in your values

# Stable Diffusion API
SD_API_URL=http://localhost:7860
SD_API_KEY=voting-app:your-api-key-here
SD_API_TIMEOUT=600

# FastAPI Wrapper
API_HOST=0.0.0.0
API_PORT=8080
API_KEY_SECRET=your-secret-key-here

# AWS S3
AWS_REGION=ap-southeast-2
S3_BUCKET_IMAGES=voting-app-ai-images
S3_BUCKET_MODELS=voting-app-ai-models
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Database
DATABASE_URL=sqlite:///./ai_generation.db

# Generation Settings - Optimized for SDXL
DEFAULT_STEPS=35
DEFAULT_SAMPLER=DPM++ 2M SDE Karras
DEFAULT_CFG_SCALE=5.0
DEFAULT_WIDTH=1024
DEFAULT_HEIGHT=1024

# Training Settings - Optimized for SDXL
LORA_TRAINING_STEPS=2000
LORA_LEARNING_RATE=1e-5
LORA_BATCH_SIZE=1
LORA_NETWORK_DIM=64
LORA_NETWORK_ALPHA=32

# Queue Settings
MAX_CONCURRENT_GENERATIONS=2
GENERATION_TIMEOUT=300
RETRY_ATTEMPTS=3