{
  "name": "voting-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 8084",
    "test": "jest",
    "test:unit": "jest --testPathPattern='\\.(test|spec)\\.(ts|tsx)$' --coverage",
    "test:integration": "jest --testPathPattern='integration.*\\.(test|spec)\\.(ts|tsx)$'",
    "test:security": "jest --testPathPattern='security.*\\.(test|spec)\\.(ts|tsx)$'",
    "test:e2e": "detox test --configuration ios",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "build:web": "expo export --platform web",
    "build:ios": "eas build --platform ios --profile preview",
    "build:android": "eas build --platform android --profile preview",
    "dev:app": "expo start",
    "dev:admin": "echo 'Admin dashboard not implemented yet'",
    "dev:backend": "echo 'Backend not implemented yet'",
    "test:app": "jest --testPathPattern=src/",
    "test:admin": "jest --testPathPattern=admin/",
    "deploy:test": "echo 'Deploy to test not implemented yet'",
    "deploy:prod": "echo 'Deploy to prod not implemented yet'",
    "cdk:diff": "echo 'CDK not implemented yet'",
    "cdk:deploy": "echo 'CDK not implemented yet'",
    "web:dev": "node start-web.js",
    "web:clean": "expo start --web --clear --port 8084",
    "detox:build:ios": "detox build --configuration ios",
    "detox:test:ios": "detox test --configuration ios"
  },
